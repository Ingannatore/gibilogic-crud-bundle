{
    "name": "gibilogic/crud-bundle",
    "type": "symfony-bundle",
    "description": "GiBiLogic CRUD Bundle",
    "keywords": [ "crud" ],
    "homepage": "http://www.gibilogic.com",
    "license": "MIT",
    "authors": [
        {
            "name":     "GiBilogic Snc",
            "homepage": "http://www.gibilogic.com",
            "email":    "info@gibilogic.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": ">=2.7",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle": ">=1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8"
    },
    "autoload": {
        "psr-4": { "Gibilogic\\CrudBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Gibilogic\\CrudBundle\\Tests\\": "tests/" }
    }
}
